setcpm(120/4)
$: note("<[c2 c3]*4 [bb1 bb2]*4 [f2 f3]*4 [eb2 eb3]*4>")
  .sound("gm_acoustic_bass, gm_electric_guitar_muted")
  .lpf(sine.range(500, 2000))
  .pan(sine.range(0, 1))
  .gain(.1)
  ._punchcard()

$: note("c2, eb3 g3 [bb3 c4]")
  .sound("gm_acoustic_bass, gm_electric_guitar_muted")
  .slow("0.5, 1, 2")
  .attack(.01)
  .gain(1)
  ._punchcard()

$: note("c2, eb3 g3 [bb3 c4]")
  .sound("piano")
  .slow("0.5, 1, 2")
  .attack(.1)
  .gain(.2)
  ._punchcard()
